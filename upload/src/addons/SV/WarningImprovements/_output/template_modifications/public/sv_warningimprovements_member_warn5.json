{
    "template": "member_warn",
    "description": "Setup default content action",
    "execution_order": 1,
    "enabled": true,
    "action": "preg_replace",
    "find": "#(<xf:radiorow name=\"content_action\" value=)(\"\")#si",
    "replace": "$1\"{{ $user.getWarningDefaultContentAction() }}\""
}